#include <iostream>
#include<algorithm>
using namespace std;
 bool compare(int a, int b)
    {
        return a<=b;
    }
int main() {
   
    int arr[]={1,2,5,10,20,50,100,200,500,2000};
    int n=sizeof(arr)/sizeof(int);
    int money;
    cin>>money;
    while(money>0)
    {
    int lb=lower_bound(arr,arr+n,money,compare)-arr;   //subtracting 1 from lower bound
    int m=arr[lb];
    cout<<m<<",";
    money=money-m;
    }
return 0;

}
