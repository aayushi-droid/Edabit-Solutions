#include<iostream>
using namespace std;
int bsearch(int a[], int key, int n)
{    int s=0;
     int e=n-1;
    while(s<=e)
    { 
        int mid=(s+e)/2;
        if(a[mid]==key)
        {
            return mid;
        }
        else
        if(a[mid]<key)
        {
            s=mid+1;
        }
        else
        if(a[mid]>key)
        {
            e=mid-1;
        }
    }
        return -1;
}
int main()
{
int a[100],key,n;
cin>>n;
for(int i=0;i<n;i++)
{
    cin>>a[i];
}
cin>>key;
cout<<bsearch(a,key,n);
return 0;
}
